<div class= "container ">
  <table class= "table">
      <thead>
        <tr>
          <th scope="col">Project Title</th>
          <th scope="col">Description</th>
          <th scope="col">Actions</th>
          <th scope="col">Assignee to Developer</th>
        </tr>
      </thead>
      <tbody>
        <% Project.all.each do |project|%>
          <tr>
            <td>
              <%=project.title%>
            </td>
            <td>
              <%=project.description%>
            </td>
            <td>
              <%=link_to "", edit_project_path(project), class: "fa-solid fa-pen-to-square" %>
              <%=button_to "", project_path(project), method: :delete, class: "fa-solid fa-trash" %>
            </td>
        </tr>
        <%end%>
      </tbody>
  </table>
</div>
